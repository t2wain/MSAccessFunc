{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "OdbcDsn": "[DSN Name][;Password]",
    "OdbcDsnFile": "[DSN file path][;Password]",
    "OdbcDsnless": "ODBC;[connection string]",
    "OdbcDsnlessOracle": "ODBC;Driver={Oracle in instantclient_12_1};Dbq=[tnsname];Uid=xxx;Pwd=xxx;",
    "OdbcDsnlessOracle2": "ODBC;Driver={Oracle in instantclient_12_1};DBQ=[host]:[port]/[sid or service name];UID=xxx;PWD=xxx;",
    "MSAccess": "[File path][;Pwd=XXXX]",
    "Excel": "[File path]"
  },
  "AllowedHosts": "*"
}